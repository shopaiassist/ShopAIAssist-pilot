{
  "name": "chatmanager-mfe-client",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.12.x <21",
    "npm": ">=10.5.x"
  },
  "scripts": {
    "build": "echo \"Building...\" && webpack --mode production && npm run build:storybook",
    "build:dev": "webpack --mode development",
    "build:start": "cd dist && ../node_modules/.bin/serve",
    "build:storybook": "echo \"Building Storybook...\" && storybook build --test",
    "clean": "rm -rf node_modules",
    "dev:precommit": "npm run build && npm run build:storybook && npm run test && npm run dev:storybook",
    "dev:storybook": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build:storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test:storybook\"",
    "heroku-postbuild": "webpack --mode production",
    "start": "node dev-server.js",
    "start:live": "node dev-server.js --live",
    "storybook": "storybook dev -p 6006",
    "storybook:noopen": "storybook dev -p 6006 --no-open",
    "test": "node --no-deprecation ./node_modules/.bin/jest --watch",
    "test:windows": "node ./node_modules/jest/bin/jest.js",
    "test:ci": "npm run test -- --ci --coverage=false --bail --no-watch",
    "test:storybook": "test-storybook"
  },
  "license": "MIT",
  "devDependencies": {
    "react": "1.1.23",
    "@storybook/addon-essentials": "^7.6.15",
